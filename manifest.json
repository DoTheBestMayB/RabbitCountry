{
    "manifest_version": 3,
    "name": "당근나라",
    "description": "Erase Dealer!",
    "version": "1.1",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "scripting",
      "storage",
      "webNavigation"
    ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "host_permissions": [
    "*://cafe.naver.com/joonggonara*",
    "*://cafe.naver.com/joonggonara#*"
  ]
}